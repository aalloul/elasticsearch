{
  "template": "offers-v01*",
  "settings": {
    "index": {
      "number_of_shards": "6",
      "number_of_replicas": "0",
      "refresh_interval": "10s"
    }
  },
  "mappings": {
    "events": {
      "dynamic": true,
      "_all": { "enabled": "false" },
      "dynamic_templates": [
        {
          "headers": {
            "mapping": {
              "index": "yes",
              "type": "keyword"
            },
            "match_mapping_type": "keyword",
            "path_match": "headers.*"
          }
        },
        {
          "template_1": {
            "mapping": {
              "index": "not_analyzed",
              "index_options": "docs",
              "norms": {"enabled": false},
              "type": "keyword"
            },
            "match_mapping_type": "keyword",
            "match": "*"
          }
        }
      ],
      "properties": {
        "headers": {
          "enabled": true,
          "type": "object"
        },
        "requestArrived":{
          "type": "date",
          "index_options": "docs",
          "format": "strict_date_optional_time||epoch_millis"
        },
        "requestTime":{
          "type": "date",
          "index_options": "docs",
          "format": "strict_date_optional_time||epoch_millis"
        },
